    "## Clustering Inputs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "e20d6f6d",
   "metadata": {},
   "outputs": [],
   "source": [
    "clust_columns = [\"police_spending_as_a_percent_2012\", \"violent_crimes_per_10000_residents_2012\", \"gun_licenses_per_1000_residents_2012\"]\n",
    "\n",
    "num_clusts = 5\n",
    "\n",
    "linkage_type = \"average\"\n",
    "\n",
    "df_cluster = df_all[clust_columns].dropna()\n",
    "\n",
    "scaler = StandardScaler()\n",
    "scaled_data = scaler.fit_transform(df_cluster)\n",
    "agglom = AgglomerativeClustering(n_clusters=num_clusts, linkage=linkage_type).fit(scaled_data)\n",
    "df_cluster[\"label\"] = agglom.labels_\n"
   ]
